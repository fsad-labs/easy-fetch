{
  "name": "@fsad-labs/easy-fetch",
  "author": "fullstack-ad",
  "description": "Lightweight fetch wrapper with middleware support",
  "keywords": ["fetch","middleware","http","typescript","api"],
  "license": "MIT",
  "files": ["dist"],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsad-labs/easy-fetch"
  },
  "version": "1.0.6",
  "main": "dist/index.cjs",
  "module": "./dist/index.js",
  "types":"dist/index.d.ts",
  "exports":{
    ".":{
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "dev":"tsup src/index.ts --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
